var common_ops={
    init:function () {
        this.eventBind();
    },


    eventBind:function () {

    },

    WebUrl:function(path,params){
        var url = path;
        var _paramUrl = '';
        if( params ){
            _paramUrl = Object.keys(params).map(function(k) {
                return [encodeURIComponent(k), encodeURIComponent(params[k])].join("=");
            }).join('&');
            _paramUrl = "?"+_paramUrl;
        }
        return url + _paramUrl

    },


};


$(function () {
    common_ops.init();
});
